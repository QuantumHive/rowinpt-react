{
  "name": "rowinpt",
  "version": "1.1.5",
  "description": "Rowin Personal Trainer WebApp",
  "author": "QuantumHive",
  "license": "MIT",
  "engines": {
    "npm": ">=3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QuantumHive/rowinpt-react"
  },
  "scripts": {
    "prewebpack": "rimraf ./wwwroot/dist",
    "webpack": "webpack --config webpack.config.dev.babel.js",
    "prebuild": "rimraf ./wwwroot/dist",
    "build": "webpack --config webpack.config.prod.babel.js --env.prod"
  },
  "dependencies": {
    "axios": "0.16.2",
    "bootstrap": "4.0.0-alpha.6",
    "chart.js": "2.6.0",
    "font-awesome": "4.7.0",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "query-string": "^4.3.4",
    "react": "15.6.1",
    "react-chartjs-2": "2.1.0",
    "react-dom": "15.6.1",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.1",
    "react-spinkit": "3.0.0",
    "redux": "3.7.1",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "aspnet-webpack": "1.0.29",
    "autoprefixer": "7.1.2",
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.11",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.5.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.4",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "node-sass": "4.5.3",
    "postcss-loader": "2.0.6",
    "prop-types": "15.5.10",
    "redux-immutable-state-invariant": "2.0.0",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "3.1.0"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ],
    "env": {
      "development": {
        "presets": [
          "env"
        ]
      },
      "production": {
        "presets": [
          [
            "env",
            {
              "es2015": {
                "modules": false
              }
            }
          ]
        ],
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-remove-prop-types"
        ]
      }
    }
  }
}
